      <name>SQL - Criar Tabela Pontos Finais Efetivos</name>
      <description/>
      <type>SQL</type>
      <sql>CREATE SEQUENCE IF NOT EXISTS stage.sq_pf_efetivos&#x3b;&#xa;CREATE TABLE IF NOT EXISTS stage.pontos_finais_efetivos&#xa;&#x28;&#xa;  id integer NOT NULL DEFAULT nextval&#x28;&#x27;stage.sq_pf_efetivos&#x27;&#x3a;&#x3a;regclass&#x29;,&#xa;  linha text,&#xa;  nome_linha text,&#xa;  sentido text,&#xa;  direcao double precision,&#xa;  ordem text,&#xa;  datahora timestamp without time zone,&#xa;  ponto_partida integer,&#xa;  distancia numeric,&#xa;  latitude double precision,&#xa;  longitude double precision,&#xa;  velocidade double precision,&#xa;  CONSTRAINT pontos_finais3_pkey PRIMARY KEY &#x28;id&#x29;&#xa;&#x29;&#x3b;</sql>
