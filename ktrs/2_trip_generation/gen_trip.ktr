    <sql>DROP SEQUENCE temp.id_viagem&#x3b;&#xa;CREATE SEQUENCE temp.id_viagem&#x3b;&#xa;DROP TABLE temp.viagens CASCADE&#x3b;&#xa;CREATE TABLE temp.viagens AS&#x28;&#xa;    SELECT&#xa;  nextval&#x28;&#x27;temp.id_viagem&#x27;&#x29; id_viagem, &#xa;  ordem,&#xa;  linha,&#xa;  sentido,&#xa;  nome_linha,&#xa;  direcao,&#xa;  hora_partida,&#xa;  duracao_viagem,&#xa;  distancia&#x2f;EXTRACT&#x28;EPOCH FROM &#x28;duracao_viagem&#x29;&#x2f;3600&#x29; kmh&#xa;    FROM &#x28;&#xa;  SELECT&#xa;    ordem,&#xa;    linha,&#xa;    sentido,&#xa;    nome_linha,&#xa;    direcao,&#xa;    datahora_acima hora_partida,&#xa;    datahora hora_chegada, &#x28;datahora - datahora_acima&#x29; as duracao_viagem,&#xa;    distancia&#xa;  FROM &#x28;&#xa;  SELECT &#x2a;, lag&#x28;sentido, 1&#x29; over &#x28;order by ordem, datahora ASC&#x29; direcao_acima, lag&#x28;ordem, 1&#x29; over &#x28;order by ordem, datahora ASC&#x29; ordem_acima,&#xa;  lag&#x28;datahora, 1&#x29; over &#x28;order by ordem, datahora ASC&#x29; datahora_acima&#xa;  FROM temp.pontos_finais_efetivos&#xa;  &#x29;b&#xa;  WHERE&#xa;  sentido &#x3c;&#x3e; direcao_acima AND&#xa;  ordem &#x3d; ordem_acima AND&#xa;  EXTRACT&#x28;EPOCH FROM &#x28;datahora - datahora_acima&#x29;&#x2f;3600&#x29; &#x3e; &#x28;distancia &#x2f; 60&#x29; AND&#xa;  EXTRACT&#x28;EPOCH FROM &#x28;datahora - datahora_acima&#x29;&#x2f;3600&#x29; &#x3c; &#x28;distancia &#x2f; 8&#x29;&#xa;  &#x29;b&#xa;&#x29;</sql>
